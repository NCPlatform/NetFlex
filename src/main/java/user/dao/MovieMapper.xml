<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="movieSQL">
	<select id="movie" parameterType="movie">
 		select * from movie
 	</select>
 	
 	<select id="movieList" parameterType="java.util.Map" resultType="movie">
		<!-- MySQL : select * from usertable id limit 시작위치, 개수 (시작 위치는 0부터 시작한다) -->
		select * from movietable id limit 0, 3
 	</select>
 </mapper>